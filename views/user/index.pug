extends layout/base.pug

block title 
    <title>Trang chủ</title>

block header 
    <link rel="stylesheet" href="css/mycss/index.css">

block content
    // Hero Section Begin
    section.hero
        .container
            .row
                .col-lg-12
                    #carouselExampleIndicators.carousel.slide(data-ride='carousel')
                        ol.carousel-indicators
                            li.active(data-target='#carouselExampleIndicators' data-slide-to='0')
                            li(data-target='#carouselExampleIndicators' data-slide-to='1')
                            li(data-target='#carouselExampleIndicators' data-slide-to='2')
                        .carousel-inner
                            .carousel-item.active.banner-item
                                img.d-block.w-100(src='./img/NhaTrangTP.jpg' alt='...')
                                .hero__text.banner-text(style='top:10%; right: 10%;')
                                    span FRUIT FRESH
                                    h2
                                        | Vegetable 
                                        br
                                        | 100% Organic
                                    p Free Pickup and Delivery Available
                                    a.primary-btn(href='#') SHOP NOW
                            .carousel-item.banner-item
                                img.d-block.w-100(src='./img/thanhpho.jpg' alt='...')
                                .hero__text.banner-text(style='top:10%; right: 10%;')
                                    span FRUIT FRESH
                                    h2
                                        | Vegetable 
                                        br
                                        | 100% Organic
                                    p Free Pickup and Delivery Available
                                    a.primary-btn(href='#') SHOP NOW
                            .carousel-item.banner-item
                                img.d-block.w-100(src='./img/sapa.jpg' alt='...')
                                .hero__text.banner-text(style='top:10%; right: 10%;')
                                    span FRUIT FRESH
                                    h2
                                        | Vegetable 
                                        br
                                        | 100% Organic
                                    p Free Pickup and Delivery Available
                                    a.primary-btn(href='#') SHOP NOW
                        a.carousel-control-prev(href='#carouselExampleIndicators' role='button' data-slide='prev')
                            span.carousel-control-prev-icon(aria-hidden='true')
                            span.sr-only Previous
                        a.carousel-control-next(href='#carouselExampleIndicators' role='button' data-slide='next')
                            span.carousel-control-next-icon(aria-hidden='true')
                            span.sr-only Next
    // Hero Section End
    
    // Modal
    #exampleModal.modal.fade(tabindex='-1' aria-labelledby='exampleModalLabel' aria-hidden='true')
        .modal-dialog
            .modal-content
                .modal-header
                    h5#exampleModalLabel.modal-title Xin chào Quý khách
                    button.close(type='button' data-dismiss='modal' aria-label='Close')
                        span(aria-hidden='true') &times;
                .modal-body
                    ul.nav.nav-pills(style='padding: 7px 0px 21px 3%;')
                        li.nav-item
                            a.nav-link.active(style='margin-right: 5px;' href='#' onclick="CustomerLogin(this, 'loginTabPane')") &Dstrok;&abreve;ng nh&#x1EAD;p
                        li.nav-item
                            a.nav-link(href='#' onclick="CustomerLogin(this, 'regTabPane')") &Dstrok;&abreve;ng k&yacute;
                    .tab-content
                        #loginTabPane.tab-pane.active
                            form.form-horizontal(role='form')
                                .form-group
                                    .col-sm-12
                                        label(for='username') T&ecirc;n &dstrok;&abreve;ng nh&#x1EAD;p
                                        input#username.form-control(placeholder='Nhập tên đăng nhập ...' type='text')
                                .form-group
                                    .col-sm-12
                                        label(for='password') M&#x1EAD;t kh&#x1EA9;u
                                        input#password.form-control(placeholder='Nhập mật khẩu ...' type='password')
                                .form-group(style='margin-top: 27px;')
                                    .col-sm-10
                                        button.btn.btn-primary.btn-radius.btn-brd.grd1(type='submit')
                                            | &Dstrok;&abreve;ng nh&#x1EAD;p
                                        a.for-pwd.btn.btn-link(href='javascript:;') Forgot your password?
                        #regTabPane.tab-pane
                            form.form-horizontal(role='form')
                                .form-group
                                    .col-sm-12
                                        label(for='hoTen') H&#x1ECD; t&ecirc;n
                                        input#hoTen.form-control(placeholder='Nhập tên ...' type='text')
                                .form-group
                                    .col-sm-12
                                        label(for='sdt') S&#x1ED1; &dstrok;i&#x1EC7;n tho&#x1EA1;i
                                        input#sdt.form-control(placeholder='Nhập số điện thoại ...' type='text')
                                .form-group
                                    .col-sm-12
                                        label(for='email') Email
                                        input#email.form-control(placeholder='Nhập email ...' type='email')
                                .form-group
                                    .col-sm-12
                                        label(for='username') T&ecirc;n &dstrok;&abreve;ng nh&#x1EAD;p
                                        input#username.form-control(placeholder='Nhập tên đăng nhập ...' type='text')
                                .form-group
                                    .col-sm-12
                                        label(for='pass1') M&#x1EAD;t kh&#x1EA9;u
                                        input#pass1.form-control(placeholder='Nhập mật khẩu ...' type='password')
                                .form-group
                                    .col-sm-12
                                        label(for='pass2') X&aacute;c nh&#x1EAD;n m&#x1EAD;t kh&#x1EA9;u
                                        input#pass2.form-control(placeholder='Nhập lại mật khẩu ...' type='password')
                                .form-group(style='margin-top: 27px;')
                                    .col-sm-12
                                        button.btn.btn-primary.btn-radius.btn-brd.grd1(type='button')
                                            | &Dstrok;&abreve;ng k&yacute;
                                        button.btn.btn-light.btn-radius.btn-brd.grd1(type='reset')
                                            | L&agrave;m l&#x1EA1;i
                .modal-footer
                    button#closeLoginModal.btn.btn-secondary(type='button' data-dismiss='modal') Close
                    // <button type="button" class="btn btn-primary" onclick="CloseModal()">Save changes</button>
    // Modal End
    
